  /* 快应用特有相关 */
  "quickapp": {},
  /* 网页端特有配置 */
  "h5": {
    "router": {
      // 路由基础路径。默认为 /
      "base": "./"
    }
  },
  /* 小程序特有相关 */
  "mp-weixin": {
    "appid": "wx670f03419bc5a7c3",
    "setting": {
      "minified": true,
      "urlCheck": true
    },
    // 启用组件按需注入
    "lazyCodeLoading": "requiredComponents",
    "usingComponents": true,
    // 接口权限相关配置 https://developers.weixin.qq.com/miniprogram/dev/framework/open-ability/authorize.html#scope-%E5%88%97%E8%A1%A8
    "permission": {
      "scope.userLocation": {
        "desc": "你的位置信息将用于小程序位置接口的效果展示"
      }
    },
    // 使用以下8个地理位置相关接口时，需要声明该字段，否则将无法正常使用 https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#requiredPrivateInfos
    "requiredPrivateInfos": [
      "chooseLocation", // 打开地图选择位置
      "getLocation", // 获取精确地理位置
      "onLocationChange", // 监听实时地理位置变化事件
      "startLocationUpdate", // 接收位置消息（前台）
      "startLocationUpdateBackground", // 接收位置消息（前后台）
      "choosePoi" // 打开POI列表选择位置
      // "chooseAddress", // 获取用户地址信息
      // "getFuzzyLocation" // 获取模糊地理位置
    ]
  },
  "mp-alipay": {
    "usingComponents": true
  },
  "mp-baidu": {
    "usingComponents": true
  },
  "mp-toutiao": {
    "usingComponents": true
  },
  "uniStatistics": {
    "enable": false
  },
  "vueVersion": "3"
}